{
  "name": "Extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs", "activeTab", "storage", "contextMenus", "webNavigation", "https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["content.js"]
}
